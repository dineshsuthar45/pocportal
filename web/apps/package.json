  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev vite",
    "start:qa": "env-cmd -f .env.qa vite",
    "start:prod": "env-cmd -f .env.production vite",
    "build": "tsc && vite build",
